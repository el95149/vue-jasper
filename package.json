{
  "private": false,
  "name": "vue-jasper",
  "version": "1.0.10",
  "description": "A Vue component, based on Element UI, to consume Jasper server reports",
  "main": "./dist/vue-jasper.js",
  "scripts": {
    "build": "rimraf ./dist && webpack --config ./webpack.config.js --mode development",
    "test": "jest"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/el95149/vue-jasper.git"
  },
  "keywords": [
    "vue.js",
    "element-ui",
    "jasper"
  ],
  "author": "Angelos Anagnostopoulos <angelos@anagnostic.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/el95149/vue-jasper/issues"
  },
  "homepage": "https://github.com/el95149/vue-jasper#readme",
  "dependencies": {
    "axios": "^0.18.1",
    "chart.js": "^2.8.0",
    "csvtojson": "^2.0.10",
    "vue-chartjs": "^3.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "bili": "^4.7.3",
    "element-ui": "^2.8.2",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "rimraf": "^2.6.3",
    "rollup-plugin-vue": "^5.0.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue": "^2.6.10",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
